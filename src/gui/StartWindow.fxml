<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="StartPane" prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.GUIController">
   <children>
      <ImageView fitHeight="350.0" fitWidth="400.0" opacity="0.6" pickOnBounds="true">
         <image>
            <Image url="@../../Images/Whats-Cooking-Background.png" />
         </image>
      </ImageView>
      <Label layoutX="155.0" layoutY="140.0" text="user">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <Label layoutX="165.0" layoutY="225.0" text="admin">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="btnUser" layoutX="50.0" layoutY="135.0" mnemonicParsing="false" onAction="#handle" opacity="0.1" prefHeight="80.0" prefWidth="300.0" style="-fx-background-color: black;">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button fx:id="btnAdmin" layoutX="50.0" layoutY="223.0" mnemonicParsing="false" onAction="#handle" opacity="0.3" prefHeight="40.0" prefWidth="300.0" style="-fx-background-color: black;">
